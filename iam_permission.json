{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "ec2:*",
        "elasticloadbalancing:*",
        "iam:PassRole",
        "iam:GetRole",
        "iam:ListRoles",
        "iam:CreateServiceLinkedRole",
        "cloudwatch:*",
        "logs:*"
      ],
      "Resource": "*"
    }
  ]
}